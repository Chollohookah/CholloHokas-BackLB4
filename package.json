{
  "name": "chollo-hooka-back",
  "version": "0.0.1",
  "description": "Backend para chollohooka",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "start:watch": "tsc-watch --target es2017 --outDir ./dist --onSuccess \"node .\"",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t chollo-hooka-back .",
    "docker:run": "docker run -p 3000:3000 -d chollo-hooka-back",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "dev": "nodemon server/server.js --watch common --watch server"
  },
  "repository": {
    "type": "git"
  },
  "author": "Tihomir Stoychev <tihomir_alcudia3@hotmail.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.2.0",
    "@loopback/authentication-jwt": "^0.8.1",
    "@loopback/authentication-passport": "^3.1.1",
    "@loopback/authorization": "^0.8.1",
    "@loopback/boot": "^3.3.1",
    "@loopback/core": "^2.15.1",
    "@loopback/repository": "^3.5.1",
    "@loopback/rest": "^9.2.1",
    "@loopback/rest-explorer": "^3.2.1",
    "@loopback/security": "^0.4.1",
    "@loopback/service-proxy": "^3.1.1",
    "@types/filesystem": "0.0.29",
    "@types/moment": "^2.13.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "file-system": "^1.2.2",
    "isemail": "^3.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "loopback-connector-mongodb": "^5.4.0",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.3.1",
    "@loopback/eslint-config": "^10.1.1",
    "@loopback/testlab": "^3.3.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.168",
    "@types/md5": "^2.2.1",
    "@types/node": "^10.17.56",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "eslint": "^7.23.0",
    "nodemon": "^2.0.6",
    "source-map-support": "^0.5.19",
    "tsc-watch": "^4.2.9",
    "typescript": "~4.2.3"
  }
}
